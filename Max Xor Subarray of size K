class Solution {
  public:
    int maxSubarrayXOR(vector<int>& arr, int k) {
        // code here
        
        int curr_xor=0;
        for(int i=0;i<k;i++){
            curr_xor^=arr[i];
        }
        
        int max_xor=curr_xor;
        for(int i=k;i<arr.size();i++){
            curr_xor^=arr[i-k];
            curr_xor^=arr[i];
            if(curr_xor> max_xor){
                max_xor=curr_xor;
            }
            
        }
        return max_xor;
        
        
    }
};
